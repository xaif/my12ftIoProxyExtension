{
    "manifest_version": 3,
    "name": "12ft.io Proxy",
    "description": "Bypass article paywalls with a single click using 12ft.io.",
    "version": "1.0",
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["activeTab"],
    "action": {
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  